{
  "name": "nestjs-prisma",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "cross-env NODE_ENV=local nest start --watch",
    "start:debug": "cross-env NODE_ENV=local nest start --debug --watch",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:prod": "cross-env NODE_ENV=prod node -r tsconfig-paths/register dist/src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "npm run lint:es && npm run format",
    "lint:es": "eslint \"src/**/*.{js,ts}\" \"test/**/*.{js,ts}\" --fix --report-unused-disable-directives --no-error-on-unmatched-pattern",
    "format": "prettier --write \"src/**/*.{js,ts}\" \"test/**/*.{js,ts}\" --no-error-on-unmatched-pattern",
    "prisma:gen": "prisma generate",
    "prisma:push": "dotenv -e .env.local -- prisma db push && dotenv -e .env.local -- npm run prisma:schema-extensions",
    "prisma:push:dev": "dotenv -e .env.dev -- prisma db push && dotenv -e .env.dev -- npm run prisma:schema-extensions",
    "prisma:push:f": "dotenv -e .env.local -- prisma db push --force-reset && dotenv -e .env.local -- npm run prisma:schema-extensions",
    "prisma:push:f:dev": "dotenv -e .env.dev -- prisma db push --force-reset && dotenv -e .env.dev -- npm run prisma:schema-extensions",
    "prisma:schema-extensions": "ts-node -r tsconfig-paths/register src/database/prisma/schema-extensions.ts",
    "prisma:seed": "dotenv -e .env.local -- prisma db seed",
    "prisma:seed:dev": "dotenv -e .env.dev -- prisma db seed",
    "prisma:reset": "dotenv -e .env.local -- prisma migrate reset --force && dotenv -e .env.local -- npm run prisma:schema-extensions",
    "prisma:reset:dev": "dotenv -e .env.dev -- prisma migrate reset --force && dotenv -e .env.dev -- npm run prisma:schema-extensions",
    "prisma:mig": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:mig:dev": "dotenv -e .env.dev -- prisma migrate dev",
    "prisma:mig:prod": "dotenv -e .env.prod -- prisma migrate deploy",
    "prisma:st": "dotenv -e .env.local -- prisma studio",
    "prisma:st:dev": "dotenv -e .env.dev -- prisma studio",
    "prisma:st:prod": "dotenv -e .env.prod -- prisma studio",
    "update:check": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@apollo/server": "^5.1.0",
    "@as-integrations/express5": "^1.1.2",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/client": "^6.18.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.25.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.1",
    "prisma-extension-soft-delete": "^2.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.18.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "prisma": {
    "schema": "src/database/prisma/schema.prisma",
    "seed": "ts-node -r tsconfig-paths/register src/database/prisma/seed.ts"
  }
}
